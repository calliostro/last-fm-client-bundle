{
    "name": "calliostro/last-fm-client-bundle",
    "description": "Last.fm API client for Symfony 5 or Symfony 6",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.3 || ^8.0",
        "snapshotpl/last-fm-client": "^1.0",
        "symfony/config": "^5.0 || ^6.0",
        "symfony/dependency-injection": "^5.0 || ^6.0",
        "symfony/http-kernel": "^5.0 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Calliostro\\LastFmClientBundle\\": "src/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0 || ^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Calliostro\\LastFmClientBundle\\Tests\\": "tests/"
        }
    }
}
